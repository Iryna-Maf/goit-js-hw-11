{{#each this}}
  <div class='photo-card'>
    <a href='{{this.largeImageURL}}'><img
        src='{{this.webformatURL}}'
        alt='{{this.tags}}'
        loading='lazy'
        width='200'
        height='150'
      /></a>
    <div class='info'>
      <p class='info-item'>
        <b class='text-card'>Likes:</b> {{this.likes}}
      </p>
      <p class='info-item'>
        <b class='text-card'>Views:</b> {{this.views}}
      </p>
      <p class='info-item'>
        <b class='text-card'>Comments:</b> {{this.comments}}
      </p>
      <p class='info-item'>
        <b class='text-card'>Downloads:</b> {{this.downloads}} 
      </p>
    </div>
  </div>
{{/each}}